    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "planer": "^1.2.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.32.1",
    "twenty-emails": "workspace:*",
    "type-fest": "^4.1.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
